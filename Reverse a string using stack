import java.util.*;
class StackN {
    public static void reverseString(Stack<Character> s , String st){
        for(int i=0;i<st.length();i++){
            s.push(st.charAt(i));
        }
        while(!s.isEmpty()){
            System.out.print(s.pop() + " ");
        }
    }
    
    public static void main(String[] args) {
       Stack<Character> s = new Stack<>();
       String st = "abcde";
       reverseString(s,st);
       
    }
}
